<h1>Listing user_agreements</h1>

<table>
  <tr>
    <th>User</th>
    <th>Agreement</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_agreements.each do |user_agreement| %>
  <tr>
    <td><%= user_agreement.user_id %></td>
    <td><%= user_agreement.agreement_id %></td>
    <td><%= link_to 'Show', user_agreement %></td>
    <td><%= link_to 'Edit', edit_user_agreement_path(user_agreement) %></td>
    <td><%= link_to 'Destroy', user_agreement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

